# GNU 		--- GNU-Project Libraries

!include ..\makefile.def

GNUOBJS=	gregex.obj getdate.obj

objects:	$(GNUOBJS)
		@%null

gregex.obj:	gregex.c gregex.h
		$(CC) /zq $(CFLAGS) -Dpce_source $*.c
getdate.obj:	getdate.c
		$(CC) /zq $(COFLAGS) $(CIFLAGS) $*.c
getdate.c:	getdate.y
		yacc getdate.y
		mv -f y.tab.c getdate.c

clean:
		rm -f *~ a.out core $(GNUOBJS) addlib
