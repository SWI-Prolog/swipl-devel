.NOCHECK

ARCH=pc
OS=windows3.1
VERSION=4.8.4, Nov 1994

MAKE=wmake /h
CC=wcc386
CXX=wpp386
CWFLAGS=
COFLAGS=/d1
CIFLAGS=-I.. -I..\..\..
CMFLAGS=-fpc -bt=windows
CFLAGS=$(CWFLAGS) $(COFLAGS) $(CIFLAGS) $(CMFLAGS)
LD=wlink
#LDFLAGS=system win386 debug all
LDFLAGS=system win386
WST=msw

LN=copy

MKPROTO=mkproto -p
CLPROTO=grep -vw NewClass
ETAGS=etags

.c.obj:
	$(CC) /zq $(CFLAGS) $*.c

.cxx.obj
	$(CXX) /zq $(CFLAGS) $*.cxx
