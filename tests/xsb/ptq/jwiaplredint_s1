s(@('walk*',*(A)),[],[sub(\(B,@(*(B),^(j))),A,m)],0,2)
s(@('walk*',j),[],[],0,2)
s(@(@(@('in*',*(A)),^(\(B,@('walk*',*(B))))),C),[],[sub(\(D,@(*(D),^(j))),C,m),sub(\(E,exists(F,/\(@(park,F),@(*(E),F)))),A,n)],0,5)
s(@(@(@('in*',*(A)),^(\(B,@('walk*',*(B))))),^(j)),[],[sub(\(C,exists(D,/\(@(park,D),@(*(C),D)))),A,n)],0,5)
s(exists(A,/\(@(park,A),@(@(@('in*',*(A)),^(\(B,@('walk*',*(B))))),C))),[],[sub(\(D,@(*(D),^(j))),C,m)],0,5)
s(exists(A,/\(@(park,A),@(@(@('in*',*(A)),^(\(B,@('walk*',*(B))))),^(j)))),[],[],0,5)
