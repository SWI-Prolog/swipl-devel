s(@('talk*',*(A)),[],[sub(\(B,@(*(B),^(j))),A,m)],0,2)
s(@('talk*',j),[],[],0,2)
s(@(@(@(about,^(\(A,@(*(A),B)))),^(\(C,@('talk*',*(C))))),D),[],[sub(\(E,@(*(E),^(j))),D,m),sub(\(F,exists(G,/\(@(unicorn,G),@(*(F),G)))),B,n)],0,5)
s(@(@(@(about,^(\(A,@(*(A),B)))),^(\(C,@('talk*',*(C))))),^(j)),[],[sub(\(D,exists(E,/\(@(unicorn,E),@(*(D),E)))),B,n)],0,5)
s(@(@(@(about,^(\(A,exists(B,/\(@(unicorn,B),@(*(A),B)))))),^(\(C,@('talk*',*(C))))),D),[],[sub(\(E,@(*(E),^(j))),D,m)],0,5)
s(@(@(@(about,^(\(A,exists(B,/\(@(unicorn,B),@(*(A),B)))))),^(\(C,@('talk*',*(C))))),^(j)),[],[],0,5)
s(exists(A,/\(@(unicorn,A),@(@(@(about,^(\(B,@(*(B),A)))),^(\(C,@('talk*',*(C))))),D))),[],[sub(\(E,@(*(E),^(j))),D,m)],0,5)
s(exists(A,/\(@(unicorn,A),@(@(@(about,^(\(B,@(*(B),A)))),^(\(C,@('talk*',*(C))))),^(j)))),[],[],0,5)
