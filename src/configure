esac
case "$INSTALL_SCRIPT" in
    .*) INSTALL_SCRIPT="`pwd`/$INSTALL_SCRIPT" ;;
esac
case "$INSTALL_DATA" in
    .*) INSTALL_DATA="`pwd`/$INSTALL_DATA" ;;
